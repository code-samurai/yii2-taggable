{
  "name": "blitzbrands/yii2-taggable",
  "description": "Taggable behavior for the Yii framework",
  "keywords": [
    "blitzbrands",
    "yii",
    "yii2",
    "yii 2",
    "behavior",
    "tags"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/2amigos/yii2-taggable-behavior",
  "authors": [
    {
      "name": "Hamza Chouaibi",
      "email": "hamza.chouaibi@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0",
    "kartik-v/yii2-widget-select2": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpunit/dbunit": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "blitzbrands\\taggable\\": "src"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "bower-asset-library": "vendor/bower"
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
